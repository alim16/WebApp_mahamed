<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
    <style type="text/css">
    <%@include file="style1.css" %>
    </style>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Register page</title>
</head>
<body>
	<form action="registerForm" method="post">
	Username: <input name="username" type="text" placeholder="Username" required="true"/>
	<br>
	Password: <input name="password" type="password" placeholder="Password" required="true"/>
	<br>
	FirstName: <input name="firstname" type="text" placeholder="firstName" required="false"/>
	<br>
	LastName: <input name="lastname" type="text" placeholder="LastName" required="false"/>
	<br>
	Role: <input name="role" type="number" placeholder="role" required="false"/>
	<br>
	<input name="submit" type="submit" value="Register"/>
	</form>
</body>
</html>